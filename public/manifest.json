{
  "manifest_version": 3,
  "name": "Kayros",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"]
}